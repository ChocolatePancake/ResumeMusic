<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.leancloud.login">

    <application
        android:name=".LoginApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_all_model_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_all_model_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/MyAppTheme">
        <activity android:name=".activity.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.PasswordFindActivity" />
        <activity android:name=".activity.RegisterActivity" />
        <activity android:name=".activity.PasswordChangeActivity" />
        <activity android:name=".activity.PersonalActivity" />

    </application>
</manifest>