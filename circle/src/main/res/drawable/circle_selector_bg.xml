<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item  android:state_selected="true" >
        <shape >
            <solid android:color="@color/page_white"/>
            <stroke android:color="@color/colorLine" android:width="@dimen/dp_1"/>
        </shape>
    </item>

    <item  android:state_selected="false" >
        <shape >
            <solid android:color="@color/page_white"/>
            <stroke android:color="@color/text_title" android:width="@dimen/dp_1"/>
        </shape>
    </item>
</selector>

